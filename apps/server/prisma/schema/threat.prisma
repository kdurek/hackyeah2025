enum ThreatType {
  DRONE
}

model Threat {
  id             String     @id @default(ulid())
  mapCoordsLat   Float
  mapCoordsLon   Float
  localPositionX Float
  localPositionY Float
  localPositionZ Float
  type           ThreatType
  createdAt      DateTime   @default(now())
  updatedAt      DateTime   @updatedAt

  @@map("threat")
}
